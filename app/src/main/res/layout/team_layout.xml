						<CheckBox
							android:id="@+id/fast_checkbox"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"/>
					</LinearLayout>

				</LinearLayout>
				<!--End Defense-->


			</LinearLayout>

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="48dp"
				android:layout_column="0"
