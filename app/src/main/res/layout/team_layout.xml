							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/high_text_view"
							/>
						<Button
							android:layout_width="35dp"
							android:layout_height="35dp"
							android:text="+"
							android:tag="HIGH"
							android:onClick="addShotClicked"/>
